name = "collaboration-service-idl"
service_version = "1.0.0snapshot"
idl_version = "1.0snapshot"
subpackage_of = "collaboration-service == "..version
dependencies = {
  "scs-idl1.2 == 1.2snapshot",
  "openbus-idl2.0 == 2.0snapshot",
}
install_files = {
  ["idl/*.idl"] = "idl/collaboration-service-"..idl_version,
