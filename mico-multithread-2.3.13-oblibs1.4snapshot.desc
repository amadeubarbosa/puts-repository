name = "mico-multithread"
version = "2.3.13-oblibs1.4snapshot"
url = SVNREPURL .. "/openbus/libs/trunk/mico-2.3.13"
build = {
  type = "autotools",
  Linux26_64 = "rm -f config.cache && ./configure --prefix="..TMPDIR.. " --disable-sl3 --disable-csiv2 --disable-dynamic --disable-time --disable-property --disable-events --disable-naming && make && make install",
  Linux = "rm -f config.cache && ./configure --disable-dynamic --prefix=".. TMPDIR .. " && make && make install",
  Darwin = "rm -f config.cache && ./configure --disable-dynamic --prefix=".. TMPDIR .. " && make && make install",
  MacOS = "rm -f config.cache && ./configure --disable-dynamic --prefix=".. TMPDIR .. " && make && make install",
  SunOS = 'rm -f config.cache && CC=cc CXX=CC CONF_AR=CC CONF_ARFLAGS="-m64 -xar -o" CFLAGS="-KPIC -m64" CXXFLAGS="-erroff=badargtype2w -library=stlport4 -KPIC -m64" LDFLAGS="-m64" ./configure --disable-dynamic --prefix='.. TMPDIR .. ' && make && make install',
}
install_files = {
  [TMPDIR .."/lib/libmico*.so*"] = "lib/mico-2.3.13-multithread",       --dynamic linux,aix,sunos
  [TMPDIR .."/bin/*"] = "bin/mico-2.3.13-multithread",
}
dev_files = {
  [TMPDIR .."/lib/libmico*.*"] = "lib/mico-2.3.13-multithread",       --dynamic linux,aix,sunos
  [TMPDIR .."/bin/*"] = "bin/mico-2.3.13-multithread",
  [TMPDIR .."/include/*"] = "include/mico-2.3.13-multithread",
  [SVNDIR.."/specs/shell/mico-setup-multithread*"] = "specs/shell/",
}